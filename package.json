		"docs-forOwn": "jsdoc2md src/forOwn.js > docs/forOwn.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-mapOwn": "jsdoc2md src/mapOwn.js > docs/mapOwn.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-traverse": "jsdoc2md src/traverse.js > docs/traverse.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-isEmpty": "jsdoc2md src/isEmpty.js > docs/isEmpty.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-pull": "jsdoc2md src/pull.js > docs/pull.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-isEqual": "jsdoc2md src/isEqual.js > docs/isEqual.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-deepEqual": "jsdoc2md src/deepEqual.js > docs/deepEqual.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-diffUpdate": "jsdoc2md src/diffUpdate.js > docs/diffUpdate.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-intersection": "jsdoc2md src/intersection.js > docs/intersection.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-clone": "jsdoc2md src/clone.js > docs/clone.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-combo": "jsdoc2md src/combo.js > docs/combo.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-mix": "jsdoc2md src/mix.js > docs/mix.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-powerset": "jsdoc2md src/powerset.js > docs/powerset.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-nestedEach": "jsdoc2md src/nestedEach.js > docs/nestedEach.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-repeat": "jsdoc2md src/repeat.js > docs/repeat.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-fill": "jsdoc2md src/fill.js > docs/fill.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs": "npm run docs-readme && npm run docs-get && npm run docs-has && npm run docs-set && npm run docs-unset && npm run docs-forIn && npm run docs-forOwn && npm run docs-mapOwn && npm run docs-traverse && npm run docs-isEmpty && npm run docs-isEqual && npm run docs-deepEqual && npm run docs-diffUpdate && npm run docs-pull && npm run docs-intersection && npm run docs-clone && npm run docs-combo && npm run docs-mix && npm run docs-powerset && npm run docs-nestedEach && npm run docs-repeat && npm run docs-fill"
	},
