{
	"name": "object-agent",
	"version": "1.3.0",
	"description": "A javascript library for working with objects",
	"main": "index.js",
	"sideEffects": false,
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"version": "npm run docs && git add docs",
		"docs:readme": "jsdoc2md index.js > README.md",
		"docs:get": "jsdoc2md src/get.js > docs/get.md",
		"docs:has": "jsdoc2md src/has.js > docs/has.md",
		"docs:set": "jsdoc2md src/set.js > docs/set.md",
		"docs:unset": "jsdoc2md src/unset.js > docs/unset.md",
		"docs:forIn": "jsdoc2md src/forIn.js > docs/forIn.md",
		"docs:forOwn": "jsdoc2md src/forOwn.js > docs/forOwn.md",
		"docs:forOwnReduce": "jsdoc2md src/forOwnReduce.js > docs/forOwnReduce.md",
		"docs:mapOwn": "jsdoc2md src/mapOwn.js > docs/mapOwn.md",
		"docs:traverse": "jsdoc2md src/traverse.js > docs/traverse.md",
		"docs:isEmpty": "jsdoc2md src/isEmpty.js > docs/isEmpty.md",
		"docs:pull": "jsdoc2md src/pull.js > docs/pull.md",
		"docs:isEqual": "jsdoc2md src/isEqual.js > docs/isEqual.md",
		"docs:deepEqual": "jsdoc2md src/deepEqual.js > docs/deepEqual.md",
		"docs:diffUpdate": "jsdoc2md src/diffUpdate.js > docs/diffUpdate.md",
		"docs:intersection": "jsdoc2md src/intersection.js > docs/intersection.md",
		"docs:superimpose": "jsdoc2md src/superimpose.js > docs/superimpose.md",
		"docs:clone": "jsdoc2md src/clone.js > docs/clone.md",
		"docs:combo": "jsdoc2md src/combo.js > docs/combo.md",
		"docs:mix": "jsdoc2md src/mix.js > docs/mix.md",
		"docs:powerset": "jsdoc2md src/powerset.js > docs/powerset.md",
		"docs:nestedEach": "jsdoc2md src/nestedEach.js > docs/nestedEach.md",
		"docs:repeat": "jsdoc2md src/repeat.js > docs/repeat.md",
		"docs:fill": "jsdoc2md src/fill.js > docs/fill.md",
		"docs:pathUtilities": "jsdoc2md src/utility/*.js > docs/pathUtilities.md",
		"docs": "run-p \"docs:* -- -t docs/docs.hbs --plugin dmd-readable\""
	},
	"badges": [
		{
			"name": "npm",
			"image": "https://img.shields.io/npm/v/object-agent.svg",
