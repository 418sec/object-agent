		"karma": "^4.4.1",
		"karma-brief-reporter": "0.2.1",
		"karma-chrome-launcher": "^3.1.0",
		"karma-coverage": "^2.0.1",
		"karma-coveralls": "^2.1.0",
		"karma-firefox-launcher": "^1.2.0",
		"karma-mocha": "^1.3.0",
		"karma-webpack": "^4.0.2",
		"mocha": "^6.2.2",
		"npm-run-all": "^4.1.5",
		"on-change": "^1.6.2",
		"test-runner-config": "^0.5.0",
		"wallaby-webpack": "^3.9.15",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.9"
	}
}
