{
	"name": "object-agent",
	"version": "0.3.0",
	"description": "A javascript library for working with objects",
	"main": "src/index.js",
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"patch": "npm version patch -m \"Increment patch version to %s\"",
		"minor": "npm version minor -m \"Increment minor version to %s\"",
		"major": "npm version major -m \"Increment major version to %s\"",
		"docs-readme": "jsdoc2md src/index.js > README.md --template docs.hbs --partial scope.hbs",
		"docs-get": "jsdoc2md src/get.js > docs/get.md --template docs.hbs",
		"docs-has": "jsdoc2md src/has.js > docs/has.md --template docs.hbs",
		"docs-set": "jsdoc2md src/set.js > docs/set.md --template docs.hbs",
		"docs-unset": "jsdoc2md src/unset.js > docs/unset.md --template docs.hbs",
		"docs-forIn": "jsdoc2md src/forIn.js > docs/forIn.md --template docs.hbs",
		"docs-forOwn": "jsdoc2md src/forOwn.js > docs/forOwn.md --template docs.hbs",
		"docs-mapOwn": "jsdoc2md src/mapOwn.js > docs/mapOwn.md --template docs.hbs",
		"docs-traverse": "jsdoc2md src/traverse.js > docs/traverse.md --template docs.hbs",
		"docs-isEmpty": "jsdoc2md src/isEmpty.js > docs/isEmpty.md --template docs.hbs",
		"docs-pull": "jsdoc2md src/pull.js > docs/pull.md --template docs.hbs",
		"docs-isEqual": "jsdoc2md src/isEqual.js > docs/isEqual.md --template docs.hbs",
		"docs-deepEqual": "jsdoc2md src/deepEqual.js > docs/deepEqual.md --template docs.hbs",
		"docs-diffUpdate": "jsdoc2md src/diffUpdate.js > docs/diffUpdate.md --template docs.hbs",
		"docs-intersection": "jsdoc2md src/intersection.js > docs/intersection.md --template docs.hbs",
		"docs-clone": "jsdoc2md src/clone.js > docs/clone.md --template docs.hbs",
		"docs": "npm run docs-readme && npm run docs-get && npm run docs-has && npm run docs-set && npm run docs-unset && npm run docs-forIn && npm run docs-forOwn && npm run docs-mapOwn && npm run docs-traverse && npm run docs-isEmpty && npm run docs-isEqual && npm run docs-deepEqual && npm run docs-diffUpdate && npm run docs-pull && npm run docs-intersection && npm run docs-clone"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DarrenPaulWright/object-agent.git"
	},
	"keywords": [
		"object",
		"get",
		"set",
		"has",
		"unset",
		"diff",
		"forIn",
		"forOwn",
		"same",
		"traverse",
		"isEmpty",
		"isEqual",
		"deepEqual",
		"intersection"
	],
	"author": "Darren Paul Wright <Darren@DarrenPaulWright.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/DarrenPaulWright/object-agent/issues"
	},
	"homepage": "https://github.com/DarrenPaulWright/object-agent#readme",
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.3.1",
		"ajv": "^6.9.1",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"babel-plugin-istanbul": "^5.1.0",
		"chai": "^4.2.0",
		"eslint": "^5.13.0",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-babel": "^5.3.0",
		"jsdoc-to-markdown": "^4.0.1",
		"karma": "^3.1.4",
		"karma-brief-reporter": "0.0.7",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage": "^1.1.2",
		"karma-coveralls": "^2.1.0",
		"karma-firefox-launcher": "^1.1.0",
		"karma-mocha": "^1.3.0",
		"karma-webpack": "^3.0.5",
		"mocha": "^5.2.0",
		"on-change": "^1.0.0",
		"test-runner-config": "^0.5.0",
		"wallaby-webpack": "^3.9.13",
		"webpack": "^4.29.3",
		"webpack-cli": "^3.2.3"
	}
}
