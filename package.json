{
	"name": "object-agent",
	"version": "0.5.2",
	"description": "A javascript library for working with objects",
	"main": "src/index.js",
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"patch": "npm version patch -m \"Increment patch version to %s\" && npm publish",
		"minor": "npm version minor -m \"Increment minor version to %s\" && npm publish",
		"major": "npm version major -m \"Increment major version to %s\" && npm publish",
		"docs-readme": "jsdoc2md src/index.js > README.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-get": "jsdoc2md src/get.js > docs/get.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-has": "jsdoc2md src/has.js > docs/has.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-set": "jsdoc2md src/set.js > docs/set.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-unset": "jsdoc2md src/unset.js > docs/unset.md -t docs/docs.hbs --plugin dmd-readable -d 3",
